<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.buckzy.common.service.model.CustomUser" />
	<class name="com.buckzy.common.service.model.impl.CustomUserImpl" table="BUCKZY_CustomUser">
		<id name="customUserId" type="long">
			<generator class="assigned" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="partyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="partyUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="mobileNo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobCountryCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="deviceInfo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="documentVerified" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="accountCompleted" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="profileComplete" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="socialLogin" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="restPass" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>